{
    "env": {
        "browser": true,
        "node": true
    },

    "globals": {
    },

    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
        }
    },

    "rules": {
        "block-scoped-var": 2,
        "complexity": "warn",
        "consistent-return": 2,
        "curly": 2,
        "default-case": 2,
        "dot-notation": 2,
        "eqeqeq": [2, "allow-null"],
        "guard-for-in": 2,
        "no-alert": 2,
        "no-caller": 2,
        "comma-dangle": 2,
        "no-div-regex": 2,
        "no-dupe-keys": 2,
        "no-else-return": 2,
        "no-eq-null": 0,
        "no-eval": 2,
        "no-extend-native": 2,
        "no-extra-bind": 2,
        "no-extra-boolean-cast": 2,
        "no-fallthrough": 2,
        "no-floating-decimal": 2,
        "no-implied-eval": 2,
        "no-irregular-whitespace": 2,
        "no-labels": 2,
        "no-iterator": 2,
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-multi-str": 2,
        "no-native-reassign": 2,
        "no-new": 2,
        "no-new-func": 2,
        "no-new-wrappers": 2,
        "no-octal": 2,
        "no-octal-escape": 2,
        "no-proto": 2,
        "no-redeclare": 2,
        "no-return-assign": 2,
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-use-before-define": [2, "nofunc"],
        "no-unused-expressions": 2,
        "no-unused-vars": [2, { "vars": "all", "args": "after-used" }],
        "no-unreachable": 2,
        "no-void": 2,
        "no-warning-comments": 2,
        "no-with": 2,
        "radix": 2,
        "vars-on-top": 2,
        "yoda": 2,
        "quotes": [2, "single"],
        "eol-last": [2],
        "camelcase": 0,
        "consistent-this": [2, "self"],
        "new-cap": 0,
        "new-parens": 2,
        "func-style": [2, "declaration"],
        "no-lonely-if": 2,
        "no-new-object": 2,
        "semi-spacing": 2,
        "brace-style": [2, "1tbs"],
        "no-extra-parens": [2, "functions"],
        "space-before-blocks": [2, "always"],
        "space-in-parens": [2, "never"],
        "space-infix-ops": 2,
        "indent": ["error", 4, {
            "VariableDeclarator": 2
        }],
        "no-mixed-spaces-and-tabs": 2,
        "use-isnan": 2,
        "valid-jsdoc": [2, {
            "prefer": {
                "return": "returns"
            }
        }],
        "max-nested-callbacks": [2, 3],
        "semi": [2, "always"],
        "one-var": [2, "never"],
        "no-mixed-requires": [0, false],
        "no-underscore-dangle": 0
    }
}
